NEW REPLACE  3 : `${0}.replace(${1}, ${2})`;

NEW SPLIT    2 : `${0}.split(${1})`;
NEW COUNT    2 : `${0}.value.count(${1}.value)`;
NEW GETINDEX 2 : $0 INDEX $1;

NEW FIRST    1 : GETINDEX $0 0;
NEW LAST     1 : GETINDEX $0 -1;
NEW PP       1 : $0 ADD  1;
NEW DOUB     1 : $0 MULT 2;
NEW TRIP     1 : $0 MULT 3;
NEW SQUARE   1 : $0 PWR  2;
