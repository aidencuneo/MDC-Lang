maketag : (text, printTo*) => {
    IF printTo:{'<'+printTo+'>'};
    text;
    IF printTo:{'</'+printTo+'>'};
    !ECHO;
};

!maketag 'This is a website title' 'title';
!maketag 'Hello there.' 'div';
!maketag 'This is raw text.';

args : (text, printTo*);
func : args => {
    IF printTo:{'FROM '+printTo+":\n> "};
    !ECHO text '' "\n";
};

!func 'hey';
!func 'hey' 'this place';
