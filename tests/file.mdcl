cbstrip : [str,] => {
    if (str[0] = scb) and (str[-1] = ecb) do str : str[1:-1];
    ret str.strip("\n\t ");
}

bstrip : [str,] => {
    if (str[0] = sb) and (str[-1] = eb) do str : pyeval(pretty(str) + '[1:-1]');
    ret str.strip("\n\t ");
}

